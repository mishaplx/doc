  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "cross-env DISABLED_CRON=true PLAYGROUND=true nest start --watch",
    "start:dev-full": "npm run migration:run && npm run seed:org --name=docnodebelaz6 && cross-env DISABLED_CRON=true PLAYGROUND=true nest start --watch",
    "start:devcron": "nest start --watch",
    "start:debug": "cross-env DISABLED_CRON=true nest start --debug --watch",
    "start:debugcron": "nest start --debug --watch",
    "start:prod": "npm run build && npm run config:del --name=docnodebelaz6 && (npm run migration:@generate || exit 0) && npm run migration:run && npm run config:set --name=docnodebelaz6 && npm run seed:org --name=docnodebelaz6 && cross-env NODE_ENV=production PLAYGROUND=false node dist/main",
    "start:pre-prod": "npm run build && npm run config:del --name=docnodebelaz6 && (npm run migration:@generate || exit 0) && npm run migration:run && npm run config:set --name=docnodebelaz6 && npm run seed:org --name=docnodebelaz6 && cross-env NODE_ENV=production PLAYGROUND=true node dist/main",
    "start:dev-prod": "npm run build && cross-env NODE_ENV=production PLAYGROUND=true node dist/main",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:fix": "eslint --fix --ext .js,.jsx ..",
    "test": "jest",
    "debug": "nest start",

    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",

    "typeormAbonent": "typeorm-ts-node-esm -d ./dist/database/datasource/datasource.abonent.js",
    "typeormOrgAdmin": "typeorm-ts-node-esm -d ./dist/database/datasource/datasource.org.admin.js",

    "db:create": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create",
    "db:drop": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:drop",

    "migration:generateAdmin": "npm run build && npm run typeormAdmin migration:generate src/database/migration/abonent/NextAdmin && npm run build",
    "migration:runAdmin": "npm run typeormAdmin migration:run",
    "migrateAdmin": "npm run migration:generateAdmin && npm run migration:run",
    "migration:create": "typeorm-ts-node-esm migration:create",
    "migration:@generate": "npm run typeormOrgAdmin migration:generate src/database/migration/org/Next",
    "migration:generate": "npm run build && npm run migration:@generate",
    "migration:@run": "npm run typeormOrgAdmin migration:run",
    "migration:run": "npm run build && npm run migration:@run",
    "migration:runAdminPannel": "cross-env NAME_DATABASE=$npm_config_name npm run typeormAbonent migration:run",
    "migration:go": "npm run migration:generate && npm run migration:run",
    "migration:runfake": "npm run typeormOrgAdmin migration:run --fake",
    "migration:show": "npm run typeormOrgAdmin migration:show",
    "migration:revert": "npm run typeormOrgAdmin migration:revert",

    "seed:abonent": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed --dataSource src/database/datasource/datasource.abonent.seed.ts",
    "seed:org": "cross-env NAME_DATABASE=$npm_config_name ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed --dataSource src/database/datasource/datasource.org.seed.data.ts",

    "config:set": "cross-env NAME_DATABASE=$npm_config_name ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed --dataSource src/database/datasource/datasource.org.seed.config.set.ts",
    "config:del": "cross-env NAME_DATABASE=$npm_config_name ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed --dataSource src/database/datasource/datasource.org.seed.config.del.ts",

    "@vol": "============= КОПИРОВАНИЕ ФАЙЛОВ С/НА СЕРВЕР =====================",
    "vol:local": "scp -r docnode@192.168.100.54:/home/docnode/files/docnodebelaz6/2024 /var/lib/docker/volumes/app-file-save/_data/files/docnodebelaz6/2024",
    "vol:server": "scp -r /var/lib/docker/volumes/app-file-save/_data/files/docnodebelaz6/2024 docnode@192.168.100.54:/home/docnode/files/docnodebelaz6/2024",

    "@log": "============= ЗАГРУЗКА ЛОГОВ С СЕРВЕРА =====================",
    "log": "scp -r docnode@192.168.100.54:/home/docnode/log /var/lib/docker/volumes/app-file-save/_data/log",
    "54": "ssh docnode@192.168.100.54",
    "165": "ssh docnode@192.168.105.165"
  },
  "dependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
